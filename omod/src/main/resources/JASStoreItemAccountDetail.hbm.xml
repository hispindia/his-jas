<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.jas.model">

	<class name="JASStoreItemAccountDetail" table="jas_store_item_account_detail" batch-size="500">
		<id name="id" type="int" column="id" unsaved-value="0">
				<generator class="native" />
		</id>
	    <many-to-one name="itemAccount" class="org.openmrs.module.jas.model.JASStoreItemAccount"
	        column="item_account_id" cascade="none" lazy="false" />
	    <many-to-one name="transactionDetail" class="org.openmrs.module.jas.model.JASStoreItemTransactionDetail"
	        column="transaction_detail_id" cascade="none"  />
		<property name="quantity" column="quantity"  />
	</class>
</hibernate-mapping>
